    <xsd:import schemaLocation="userdetailselements.xsd" namespace="http://schemas.data.org/2004/07/Paymen.Services"/>
    <xsd:include schemaLocation="userdetailselements.xsd" namespace="http://schemas.data.org/2004/07/Paymen.Services"/>
