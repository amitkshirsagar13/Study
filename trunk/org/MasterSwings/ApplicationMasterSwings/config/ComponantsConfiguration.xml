<?xml version="1.0" ?>
<application>
	<frame>
		<panel panelName="BASEMASTERSWINGSPANEL" subPanelName="INFOPANEL">
			<label labelName="infoLabel" labelAction="DOUBLECLICK"
				labelToolTip="InfoLabel...">
				<labelText>Label on the Info Panel of the BaseMasterSwingsPanel
				</labelText>
			</label>
		</panel>
		<panel panelName="BASEMASTERSWINGSPANEL" subPanelName="CENTERPANEL">
			<textBox textBoxName="Name" textBoxAction="FOCUSGAIN">
				<textBoxToolTip>Sample TextBox</textBoxToolTip>
			</textBox>
		</panel>
		<panel panelName="BASEMASTERSWINGSPANEL" subPanelName="BUTTONPANEL">
			<button buttonName="OK" buttonAction="OK">
				<buttonToolTip>Sample Button with OK</buttonToolTip>
			</button>
			<button buttonName="CANCEL" buttonAction="CANCEL">
				<buttonToolTip>Sample Button with CANCEL</buttonToolTip>
			</button>
			<button buttonName="RESET" buttonAction="RESET"
				buttonDisabled="Disabled">
				<buttonToolTip>Sample Button with RESET</buttonToolTip>
			</button>
		</panel>
	</frame>
</application>